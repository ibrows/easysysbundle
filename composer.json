{
    "name": "ibrows/easysys-bundle",
    "description": "Ibrows Easysys Bundle provides simple access to Easysysdata over API",
    "keywords": [
        "ibrows",
        "easysys",
        "bundle",
        "api"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Steiner",
            "email": "marc.steiner@ibrows.ch"
        },
        {
            "name": "Mike Meier",
            "email": "mike.meier@ibrows.ch"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/console": "~2.3",
        "symfony/config": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/http-kernel": "~2.3",
        "ibrows/easysys-library": "dev-master",
        "saxulum/saxulum-http-client-adapter-buzz": "~1.0,>=1.0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3"
    },
    "autoload": {
        "psr-4": {"Ibrows\\EasySysBundle\\": ""}
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@codebasehq.com:ibrows/ibrowsch/easysyslib.git"
        }
    ]
}